{
  "schemaVersion": 1,
  "id": "neodymium",
  "version": "${version}",
  "depends": {
    "fishmodloader": ">=3.2.2",
    "many-lib": ">=2.0.1"
  },
  "environment": "client",
  "name": "Neodymium",
  "description": "Neodymium is a mod that reimplements chunk rendering in MITE using modern OpenGL. This should improve performance on most hardware.",
  "authors": [
    "Makamys",
    "Arminias",
    "Limingzxc"
  ],
  "entrypoints": {
    "main": [
      "makamys.neodymium.Neodymium"
    ],
    "preLaunch": [
      "makamys.neodymium.Neodymium"
    ]
  },
  "mixins": [
    "Neodymium.mixins.json"
  ],
  "accessWidener": "Neodymium.accesswidener",
  "custom": {
    "loom:injected_interfaces": {
    }
  }
}